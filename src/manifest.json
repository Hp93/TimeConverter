{
    "manifest_version": 2,
    "name": "TimeConverter",
    "version": "1.0",
    "description": "Abc",
    "icons": {
        "48": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*",
                "file:///*"
            ],
            "js": [
                "contentscript.bundle.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "backgroundscript.bundle.js"
        ]
    },
    "applications": {
        "gecko": {
            "id": "timeconverter@hp.vn"
        }
    },
    "permissions": [
        "contextMenus",
        "notifications"
    ]
}